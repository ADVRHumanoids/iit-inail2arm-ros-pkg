<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="centauro">

<xacro:property name="model_name" value="inail2arm" />
<xacro:property name="model_version" value="0.1" />

<xacro:arg name="end_effector" default="none"/> <!-- none, ball, dagana_fixed, dagana -->
<xacro:property name="END_EFFECTOR" value="$(arg end_effector)" />

<xacro:arg name="proximity_sensors" default="false"/>
<xacro:property name="PROXIMITY_SENSORS" value="$(arg proximity_sensors)" />

</robot>
